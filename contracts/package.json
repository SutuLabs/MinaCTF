{
  "name": "minactf",
  "version": "0.1.0",
  "description": "",
  "author": "",
  "license": "Apache-2.0",
  "keywords": [
    "mina-zkapp",
    "mina-zk-app",
    "mina-dapp",
    "zkapp"
  ],
  "type": "module",
  "main": "build/src/index.js",
  "types": "build/src/index.d.ts",
  "scripts": {
    "build:ts": "tsc",
    "watch:ts": "tsc --watch",
    "build:docker": "docker build -f Dockerfile -t wizicer/ctf-server:latest .",
    "publish:docker": "docker push wizicer/ctf-server:latest",
    "dev": "nodemon server/server.ts",
    "compile": "ts-node scripts/compile.ts server/vkey.json",
    "coverage": "node --experimental-vm-modules --experimental-wasm-threads node_modules/jest/bin/jest.js --coverage",
    "format": "prettier --write --ignore-unknown **/*",
    "test": "node --experimental-vm-modules --experimental-wasm-threads node_modules/jest/bin/jest.js --verbose",
    "testw": "node --experimental-vm-modules --experimental-wasm-threads node_modules/jest/bin/jest.js --watch",
    "lint": "npx eslint src/* --fix",
    "start": "node build/src/run.js"
  },
  "lint-staged": {
    "**/*": [
      "eslint src/* --fix",
      "prettier --write --ignore-unknown"
    ]
  },
  "devDependencies": {
    "@babel/preset-env": "^7.16.4",
    "@babel/preset-typescript": "^7.16.0",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jest": "^27.0.3",
    "@types/node": "^20.3.1",
    "@typescript-eslint/eslint-plugin": "^5.5.0",
    "@typescript-eslint/parser": "^5.5.0",
    "cors": "^2.8.5",
    "cross-fetch": "^4.0.0",
    "dotenv": "^16.3.1",
    "eslint": "^8.7.0",
    "eslint-plugin-snarkyjs": "^0.1.0",
    "express": "^4.18.2",
    "jest": "^27.3.1",
    "lint-staged": "^11.0.1",
    "mina-signer": "^2.1.0",
    "nodemon": "^2.0.22",
    "pocketbase": "^0.15.2",
    "prettier": "^2.3.2",
    "snarkyjs": "^0.11.0",
    "ts-jest": "^27.0.7",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.2"
  }
}